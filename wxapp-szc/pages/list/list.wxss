}

.topNav{
  background-color: #FDE024;
  display: flex;
  align-items: center;
}

.topNav .active{
  font-size: 36rpx;
  font-weight: 400;
  color:#000000;
}

.topNav .more{
   border-left: 1px solid #dddddd;
}

.topImg{
  width:100%;
