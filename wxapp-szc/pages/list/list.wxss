.page__title{
  width: 70%;
  position: relative;
}

.page__desc{
  color: #999999;
  font-size: 30rpx;
}

.page-tit{
  position: absolute;
  left:0;
  bottom:0;
  font-size:40rpx;
  z-index: 100;
}

.page-tit image{
  width: 55rpx;
  height: 55rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  margin-right: 10rpx;
}

.page-tit text{
  padding-left: 60rpx;
  font-weight: bold;
  color:#101010;
}

.bottom-line{
  width:240rpx;
  height: 20rpx;
  background-color:#FDE024;
  border-radius: 20rpx;
  position: absolute;
  left:0;
  bottom:0;
}

.tab-nav{
  padding-left:260rpx;
  display: flex;
}

.tab-nav .hd-pro{
  width: 110rpx;
  display: block;
  font-size: 24rpx;
  text-align: center;
  color: #666666;
}

.hd_tit_pr{
  padding:10px 0;
}

.hd_tit{
  font-size:34rpx;
  line-height: 50rpx;
  margin-bottom:20rpx;
}

.hd_tit_btn {
  width:320rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.price{
  font-szie: 40rpx;
  color:#FD5107;
  margin-right:5rpx;
}

.g_price{
  color: #dddddd;
}

.hd_tit_btn .gmbtn{
   margin: 0 10rpx 0 0;
   border-radius: 60rpx;
   background-color: #FD5107;
   width: 130rpx;
   height: 45rpx;
   line-height: 1.8!important;
   font-size: 24rpx;
   color: #ffffff;
   text-align: center;
}

.title{
   width:320rpx;
   height: 30px;
   line-height: 30px;
   overflow: hidden;
   white-space: nowrap;
   text-overflow:ellipsis;
   text-align: left;
