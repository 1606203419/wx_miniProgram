}

.gonggao-icon image{
   margin-right: 5px;
   vertical-align: middle;
   width: 15px; 
   height: 15px;
}

.gonggao{
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-top:0;
}

.zxgp,.ywtj,.news,.zxhd{
