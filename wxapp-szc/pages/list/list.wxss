}

.user_meg{
  padding:15px;
}

.weui-cells:before{
  border-top:0;
}

.weui-cells::after{
  border-bottom:0;
}

.page__bd_spacing{
   padding:40rpx 0 0 0;
}

.taoxiang{
  width: 40px;
  height: 40px;
